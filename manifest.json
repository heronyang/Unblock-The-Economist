{

    "name": "Unblock The Economist",
    "description": "Reading articles on The Economist official website without limitation.",
    "version": "1.0",

    "permissions": [
        "cookies", "*://www.economist.com/*"
    ],

    "content_scripts": [
        {
            "matches": ["*://www.economist.com/*"],
            "css": ["style.css"],
            "js": ["jquery.js", "specific.js"],
            "run_at": "document_end"
        }
    ],

    "manifest_version": 2

}
